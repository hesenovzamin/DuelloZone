extends layouts/zonelayout.pug
block body
 .page-heading
  .container
    .row
      .col-md-10.offset-md-1
        h1.page-heading__title
          | Latest 
          span.highlight Results
        ol.page-heading__breadcrumb.breadcrumb
          li.breadcrumb-item
            a(href='index.html') Home
          li.breadcrumb-item
            a(href='team-overview.html') Team
          li.breadcrumb-item.active(aria-current='page') Latest Results
 // Page Heading / End
 // Team Pages Filter
 nav.content-filter
  .container
    a.content-filter__toggle(href='#')
    ul.content-filter__list
      li.content-filter__item
        a.content-filter__link(href='team-overview.html')
          small
            | The
            | Team
          | Overview
      li.content-filter__item
        a.content-filter__link(href='team-roster-1.html')
          small
            | The
            | Team
          | Roster
      li.content-filter__item
        a.content-filter__link(href='team-standings.html')
          small
            | The
            | Team
          | Standings
      li.content-filter__item.content-filter__item--active
        a.content-filter__link(href='team-last-results.html')
          small The Team
          | Latest Results
      li.content-filter__item
        a.content-filter__link(href='team-schedule.html')
          small
            | The
            | Team
          | Schedule
      li.content-filter__item
        a.content-filter__link(href='team-gallery.html')
          small
            | The
            | Team
          | Gallery
 // Team Pages Filter / End
