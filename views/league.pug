    // Team Latest Results
    .card.card--has-table
      .card__header.card__header--has-btn
        h4 Latest Results
        // Result Filter
        ul.team-result-filter
          li.team-result-filter__item
            select.form-control.input-xs
              option Playoffs 2016
              option Playoffs 2015
              option Playoffs 2014
          li.team-result-filter__item
            select.form-control.input-xs
              option December 2016
              option November 2016
              option October 2016
              option September 2016
              option August 2016
              option July 2016
              option June 2016
              option May 2016
              option April 2016
              option March 2016
              option February 2016
              option January 2016
          li.team-result-filter__item
            select.form-control.input-xs
              option Ascending
              option Descending
          li.team-result-filter__item
            button.btn.btn-default.btn-outline.btn-xs.card-header__button(type='submit')
              | Filter
              | Latest Results
